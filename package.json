    "prepush": "jest"
  },
  "dependencies": {
    "execa": "^0.8.0",
    "lodash": "^4.17.4",
    "pkg-conf": "^2.0.0",
    "z": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-config-concise": "^0.16.0",
    "eslint-config-concise-browser": "^0.16.0",
    "eslint-config-concise-esnext": "^0.16.0",
    "eslint-config-concise-jest": "^0.16.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.4.0"
  },
  "repository": "ratson/runa",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "*.md",
    "index.js",
    "cli.js",
