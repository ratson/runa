{
  "name": "runa",
  "version": "0.4.0-alpha.1",
  "description": "An extensible, daemonized process manager.",
  "bin": {
    "runa": "dist/cli.js",
    "runa-daemon": "dist/daemon-cli.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./dist/cli.js",
    "prepack": "tsc",
    "runa": "node dist/cli.js",
    "runa-daemon": "node dist/daemon-cli.js"
  },
  "dependencies": {
    "@runapm/daemon": "link:../daemon",
    "cli-table3": "^0.6.0",
    "enquirer": "^2.3.6",
    "execa": "^4.1.0",
    "exit": "^0.1.2",
    "p-queue": "^6.6.2",
    "which": "^2.0.2",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/exit": "^0.1.31",
    "@types/node": "^14.14.10",
    "@types/which": "^1.3.2",
    "@types/yargs": "^15.0.10",
    "typescript": "^4.1.2"
  },
  "repository": "ratson/runa",
  "license": "MIT",
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "*.md",
    "dist/*",
    "src/*"
  ],
  "keywords": [
    "process",
    "manager",
    "cli",
    "supervisor",
    "supervisord",
    "task",
    "runner",
    "spawn"
  ]
}
